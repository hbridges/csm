<div class="container">
  <div class="booyah-box col-xs-10 col-xs-offset-1">
    <h1>Edit Your Album</h1>
    <%= simple_form_for @album do |f| %>
      <%= f.input :name %>
      <%= f.input :description %>
      <br />
      <%= f.submit 'Update', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
